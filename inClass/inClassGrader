#include <iostream> 
using namespace std;

int main() {

    int var1;
    int var2;
    int var3;
    double grade;

    cout << "Enter your first grade: " << endl;
    cin >> var1;
    cout << "Enter your second grade: " << endl;
    cin >> var2;
    cout << "Enter your second grade: " << endl;
    cin >> var3;

    grade = (var1 + var2 + var3) / 3.0;

    cout << "Your final grade is: " << grade << endl;

return 0;
}
